{
  "manifest_version": 3,
  "name": "Regex Search",
  "description": "This extension allows you to search a page with regular expressions",
  "version": "1.0",
  "permissions": ["activeTab", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
